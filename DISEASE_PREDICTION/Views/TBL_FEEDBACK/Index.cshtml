@model IEnumerable<DISEASE_PREDICTION.Models.TBL_FEEDBACK>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a class="btn btn-primary glyphicon glyphicon-pencil" href="@Url.Action("Create","TBL_FEEDBACK")">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
           PATIENT_GENDER
        </th>
        <th>
        FEEDBACK_DETAIL
        </th>
        <th>
         FEEDBACK_EMAIL
        </th>
        <th></th>
    </tr>

@foreach (var feedback in Model) {
    <tr>
        <td>
            @feedback.PATIENT_FID
        </td>
        <td>
            @feedback.FEEDBACK_DETAIL
        </td>
        <td>
            @feedback.FEEDBACK_EMAIL
        </td>
        <td>
            <a class="btn btn-primary glyphicon glyphicon-edit" href="@Url.Action("Edit","TBL_FEEDBACK",new{ id=feedback.FEEDBACK_ID})">Edit</a>
            <a class="btn btn-primary glyphicon glyphicon-expand" href="@Url.Action("Details","TBL_FEEDBACK",new{ id=feedback.FEEDBACK_ID})">Details</a>
            <a class="btn btn-primary glyphicon glyphicon-trash" href="@Url.Action("Delete","TBL_FEEDBACK",new{ id=feedback.FEEDBACK_ID})">Delete</a>


            @*@Html.ActionLink("Edit", "Edit", new { id = item.FEEDBACK_ID }) |
            @Html.ActionLink("Details", "Details", new { id = item.FEEDBACK_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.FEEDBACK_ID })*@
        </td>
    </tr>
}

</table>
