@model IEnumerable<DISEASE_PREDICTION.Models.TBL_MEDICINECOMPANY>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    <a class="btn btn-primary glyphicon glyphicon-pencil" href="@Url.Action("Create","TBL_MEDICINECOMPANY")">Create New</a>
</p>
<table class="table">
    <tr>
        <th>
         COMPANY NAME
        </th>
        <th>
           COMPANY LOGO
        </th>
        <th>
           COMPANY LOCATION
        </th>
        <th>
        COMPANY LICENSE
        </th>
        <th></th>
    </tr>

@foreach (var company in Model) {
    <tr>
        <td>
            @company.COMPANY_NAME
        </td>
        @*<td>
            @company.COMPANY_LOGO
        </td>*@
        <td>
            @company.COMPANY_LOCATION
        </td>
        <td>
            @company.COMPANY_LICENSE
        </td>
        <td>
            <a class="btn btn-primary glyphicon glyphicon-edit" href="@Url.Action("Edit","TBL_MEDICINECOMPANY",new{ id=company.COMPANY_ID})">Edit</a>
            <a class="btn btn-primary glyphicon glyphicon-expand" href="@Url.Action("Details","TBL_MEDICINECOMPANY",new{ id=company.COMPANY_ID})">Details</a>
            <a class="btn btn-primary glyphicon glyphicon-trash" href="@Url.Action("Delete","TBL_MEDICINECOMPANY",new{ id=company.COMPANY_ID})">Delete</a>


            @*@Html.ActionLink("Edit", "Edit", new { id=company.COMPANY_ID }) |
        @Html.ActionLink("Details", "Details", new { id= company.COMPANY_ID }) |
        @Html.ActionLink("Delete", "Delete", new { id= company.COMPANY_ID })*@
        </td>
    </tr>
}

</table>
